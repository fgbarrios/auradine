    function showLine() {
      const line = document.querySelector(".products-line-container");
      const circle = line.querySelector(".circle");
      gsap.to(circle, {
        onUpdate: function () {
          if (this.progress() > 0.5) {
            line.classList.add("fixed-line");
          }
        },
      });
    }
    function hideLine() {
      const line = document.querySelector(".products-line-container");
      const circle = line.querySelector(".circle");
      gsap.to(circle, {
        onUpdate: function () {
          if (this.progress() > 0.5) {
            line.classList.remove("fixed-line");
          }
        },
      });
    }

    function moveTheLine() {
      const line = document.querySelector(".products-line-container");
      const circle = line.querySelector(".circle");
      const startSection = document.querySelector(".startLine");
      const endSection = document.querySelector(".endLine");

      if (line && startSection && endSection) {
        // Show the line when we reach the start section
        ScrollTrigger.create({
          trigger: startSection,
          start: "10% center",
          onEnter: () => {
            showLine();
          },
          onLeaveBack: () => {
            hideLine();
          },
        });

        // Hide the line when we pass the end section
        ScrollTrigger.create({
          trigger: endSection,
          start: "60% center",
          onEnter: () => {
            hideLine();
          },
          onLeaveBack: () => {
            showLine();
          },
        });
      }
    }

    function animateCircleOnSectionsEnter() {
      const line = document.querySelector(".products-line-container");
      const circle = line?.querySelector(".circle");

      let lineItems = line?.querySelectorAll(
        ".products-line-container-items li"
      );

      if (!circle) return;

      const sectionProducts = document.querySelector(
        "section[data-item='products']"
      );
      const sectionContainers = document.querySelector(
        "section[data-item='containers']"
      );
      const sectionSoftware = document.querySelector(
        "section[data-item='software']"
      );
      const sectionPartners = document.querySelector(
        "section[data-item='partners']"
      );

      const sectionsToAnimateCircle = [
        { element: sectionProducts, height: 30 },
        { element: sectionContainers, height: 45 },
        { element: sectionSoftware, height: 60 },
        { element: sectionPartners, height: 76 },
      ].filter(({ element }) => element); // filter out nulls

      ScrollTrigger.create({
        trigger: document.body,
        start: "top top",
        end: "bottom bottom",
        onUpdate: () => {
          let scrollY = window.scrollY;

          // Find the last section above the scroll position
          let current = null;
          for (let i = 0; i < sectionsToAnimateCircle.length; i++) {
            const { element } = sectionsToAnimateCircle[i];
            const offsetTop = element.offsetTop;
            if (scrollY >= offsetTop - window.innerHeight / 2) {
              current = sectionsToAnimateCircle[i];
            } else {
              break;
            }
          }

          if (current) {
            gsap.to(circle, {
              height: `${current.height}%`,
              duration: 1.2,
              ease: "power2.out",
              onUpdate: function () {
                if (this.progress() > 0.5) {
                  lineItems?.forEach((li) => {
                    li.classList.remove("active");
                    if (li.dataset.item === current.element.dataset.item) {
                      li.classList.add("active");
                    }
                  });
                }
              },
            });
          }
        },
      });
    }

    moveTheLine();
    animateCircleOnSectionsEnter();
    productsSlides();
  </script>
