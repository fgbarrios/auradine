---
import { Picture } from "astro:assets";
import Layout from "../components/Layout.astro";
import Link from "../components/Link.astro";
---

<Layout title="Support" id="support">
  <!-- Support -->

  <section class="hero about-hero relative">
    <div class="overlay absolute top-0 left-0 w-full h-full z-1">
      <img src="/support.png" class="w-full h-full object-cover" alt="" />
      <img
        src="/support-glow.png"
        class="w-full h-full object-cover absolute bottom-0"
        alt=""
      />
    </div>
    <div
      class="container mx-auto flex flex-col items-center justify-center gap-8 lg:gap-16 min-h-[500px] pt-20 md:pt-40 relative z-2"
    >
      <div class="gradient-container grandientHeading">
        <h1
          class="hero clamp-h1 font-medium leading-[99%] tracking-[-1.04px] text-center"
        >
          World-Class <br class="hidden sm:inline" />Customer Support
        </h1>
      </div>
      <div class="breadcrumps flex items-center gap-2 mt-4 text-white/60">
        <a href="/" class="hover:text-white transtion duration-300">Home</a>
        <span>/</span>
        <span>About</span>
        <span>/</span>
        <span>Support</span>
      </div>
    </div>
  </section>

  <section class="relative overflow-hidden pb-30">
    <div
      class="videoContainer z-1 absolute top-0 left-0 w-full h-full bg-[#074C80] -translate-y-[4rem]"
      style=""
    >
      <video
        src="/HB-About.mp4"
        class="w-full h-full object-cover mix-blend-color-burn opacity-60"
        autoplay
        loop
        muted
        playsinline
        webkit-playsinline
        disablepictureinpicture></video>
    </div>

    <div
      class="overlay absolute top-0 left-0 w-full h-full z-1"
      style="background: #2E93DD;
background: linear-gradient(180deg, rgba(0, 0, 0, 0) 13.23%, #080C20 69.91%);
"
    >
